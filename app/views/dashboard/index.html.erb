<header>
  <%=  javascript_include_tag "dashboard", "data-turbolinks-track" => true %>
  <%=  stylesheet_link_tag "dashboard", "data-turbolinks-track" => true %>
  <%=  image_tag "logo.png", alt: "Chennai foto wall" %>

  <%=  render 'common/flash', flash: flash %>


  <div class="filter_panel hidden" id="filter_role">
    <a href="javascript:void(0)" class="sel">Showing All Roles</a>
    <ul id="dropdown_role" class="hide">

    </ul>
  </div>

  <div class="filter_panel hidden" id="filter_project">
    <a href="javascript:void(0)" class="sel">Showing All Projects</a>
    <ul id="dropdown_project" class="hide"></ul>
  </div>
</header>

<div>
  <div id="employeeDetails" class="dashboard-layout"></div>
  <div class="clear"></div>
</div>


<div id="employeePopup">

  <div class="content_panel">
    <a href="javascript:void(0)" class="close">Close</a>

    <div class="content">

    </div>
  </div>
</div>

<script type="text/template" id="popup_template">
  <div class='empPopupData'>
    <div class='photo_panel'><img src='<%%=  consultant.photo %>' alt='<%%=  consultant.name %>'/></div>
    <div class='data_panel'>

      <label class='name'><%%=  consultant.name %></label>
      <label class='role'><%%=  consultant.role %>, </label><label class='grade'><%%=  consultant.grade %></label>

      <div class='clear'></div>

      <div class='contact'>

        <label class='email'><%%=  consultant.email %></label>
      </div>

      <label class='exp'>TW Experience/ Prior Experience :
        <span> <%%=  consultant.twexp %> /  <%%=  consultant.exp %></span></label>
      <label class='exp'>Current Project : <span> <%%=  consultant.currentproject %></span></label>

    </div>
  </div>
</script>